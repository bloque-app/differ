{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2021",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "isolatedModules": false,
    "strict": true,
    "noImplicitAny": true,
    "useUnknownInCatchVariables": false,
    "inlineSourceMap": true,
    "declaration": true
  },
  "lib": [
    "esnext"
  ]
}
