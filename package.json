{
  "name": "@bloque/differ",
  "type": "module",
  "version": "1.0.0",
  "description": "wraps objects and arrays and keeps track of global changes around wrapped objects",
  "main": "./dist/cjs/src/differ.js",
  "scripts": {
    "test": "node --loader ts-node/esm test/index.ts",
    "build": "tsc -p . && tsc -p tsconfig.cjs.json",
    "prepack": "npm run build"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/src/differ.d.ts",
        "default": "./dist/esm/src/differ.js"
      },
      "require": {
        "types": "./dist/cjs/src/differ.d.ts",
        "default": "./dist/cjs/src/differ.js"
      }
    }
  },
  "keywords": ["diff-resolution"],
  "author": "Pablo Dorado <pablo@bloque.team>",
  "license": "MIT",
  "devDependencies": {
    "@types/jsum": "^0.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "jsum": "^2.0.0-alpha.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
